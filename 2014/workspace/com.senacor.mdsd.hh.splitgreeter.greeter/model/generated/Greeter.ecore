<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="greeter" nsURI="http://www.senacor.com/mdsd/hh/splitgreeter/greeter/Greeter"
    nsPrefix="greeter">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="greetings" upperBound="-1"
        eType="#//Greeting" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Greeting">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="greetword" eType="#//Greetword"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="person" eType="ecore:EClass ../../../com.senacor.mdsd.hh.splitgreeter.persons/model/generated/Persons.ecore#//Person"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Greetword">
    <eLiterals name="DE" literal="Hallo"/>
    <eLiterals name="EN" value="1" literal="Hello"/>
    <eLiterals name="FR" value="2" literal="Bonjour"/>
  </eClassifiers>
</ecore:EPackage>
