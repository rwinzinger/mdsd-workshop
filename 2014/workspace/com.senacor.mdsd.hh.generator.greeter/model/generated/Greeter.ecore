    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lang" eType="#//Language"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Greeting">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="greetword" eType="#//Greetword"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="person" eType="#//Person"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Greetword">
    <eLiterals name="DE" literal="Hallo"/>
    <eLiterals name="EN" value="1" literal="Hello"/>
    <eLiterals name="FR" value="2" literal="Bonjour"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Language">
    <eLiterals name="DE" literal="deutsch"/>
    <eLiterals name="EN" value="1" literal="englisch"/>
