package com.senacor.mdsd.hh.xtend.test2

class Extension {
	def static toFraenkisch(String s) {
		var i=0
		var newS = ""
		
		while (i<s.length()) {
			var char nextChar = s.charAt(i)
			if (nextChar.equals(new Character('t'))) {
				nextChar = 'd'
			} else if (nextChar.equals(new Character('p'))) {
				nextChar = 'b'
			}
			i = i+1
			newS = newS+nextChar;
		}
		
		newS
	}
}