service TestMicroService {
	desc "Dies ist ein Testservice"
	
	flavour microservice
	
	operations {
	  foo secure no
	  baz secure yes
	}
}

service HazelService1 {
	desc "Hazelcast 1"
	
	flavour hazelcast
	
	operations {
	  foo secure no
	  baz secure yes
	}
}

service HazelService2 {
	desc "Hazelcast 2"
	
	flavour hazelcast
	
	operations {
	  foo secure no
	  baz secure yes
	}
}

operation foo {
	parameter INT index
	returns INT resultcode
}

operation baz {
	parameter STRING name
	returns INT resultcode
}
